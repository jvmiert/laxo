    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.1",
    "babel-plugin-formatjs": "^10.3.17",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.4",
    "postcss": "^8.4.5",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.3"
  }
