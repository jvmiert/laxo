{
  "name": "laxo-frontend",
  "private": true,
  "scripts": {
    "predev": "yarn compile",
    "dev": "next dev",
    "prebuild": "yarn compile",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract": "formatjs extract \"{pages,components,hooks,helpers}/**/*.{js,ts,tsx}\" \"--id-interpolation-pattern\" \"[sha512:contenthash:base64:6]\" \"--out-file\" \"lang/en.json\"",
