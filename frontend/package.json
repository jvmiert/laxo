{
  "name": "laxo-frontend",
  "private": true,
  "scripts": {
    "predev": "yarn compile",
    "dev": "next dev",
    "prebuild": "yarn compile",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "extract": "formatjs extract \"{pages,components,hooks,helpers}/**/*.{js,ts,tsx}\" \"--id-interpolation-pattern\" \"[sha512:contenthash:base64:6]\" \"--out-file\" \"lang/en.json\"",
    "merge": "node ./updateTranslations.js",
    "compile": "formatjs compile-folder lang compiled-lang",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "@types/final-form-focus": "^1.1.2",
    "axios": "^0.25.0",
    "final-form": "^4.20.6",
    "final-form-focus": "^1.1.2",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "17.0.2",
    "react-final-form": "^6.5.7",
    "react-intl": "^5.24.4",
    "swr": "^1.2.2",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@formatjs/cli": "^4.8.1",
    "@playwright/test": "^1.19.2",
    "@types/node": "17.0.6",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.1",
    "babel-plugin-formatjs": "^10.3.17",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.4",
    "tailwindcss": "^3.0.8",
    "typescript": "4.5.4"
  }
}
